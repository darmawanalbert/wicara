{
  "name": "@aksara/ui-kit",
  "version": "0.7.0",
  "description": "Aksara UI Kit. The component library powering Kata Platform.",
  "main": "./lib/index.js",
  "module": "./lib/index.es.js",
  "types": "./lib/index.d.ts",
  "aksara:src": "./src/index.ts",
  "repository": "https://github.com/kata-ai/wicara.git",
  "files": [
    "lib"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "yarn clean && yarn rollup",
    "clean": "rimraf lib && rimraf tsc-out",
    "prepublishOnly": "yarn build",
    "rollup": "rollup -i ./src/index.ts -c ../../rollup.config.js",
    "tsc": "tsc -b .",
    "watch": "yarn rollup -w",
    "watch:tsc": "tsc-watch -b --preserveWatchOutput"
  },
  "author": "Kata.ai <info@kata.ai>",
  "license": "Apache-2.0",
  "peerDependencies": {
    "react": "^16.8.0",
    "styled-components": "^4.2.0"
  },
  "dependencies": {
    "styled-system": "^4.2.2"
  },
  "devDependencies": {
    "@types/styled-components": "^4.1.14",
    "@types/styled-system": "^4.2.0"
  }
}
